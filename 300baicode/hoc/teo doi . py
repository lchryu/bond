    if i == 10000 :
        
        print('WILL DESTROY THIS COMPUTER IN 3 SECONDS' )
        for i in range(3, -1, -1):
            time.sleep(1)
            print(i)
        
        time.sleep(1)            
        while True :
            print('hacked')   
